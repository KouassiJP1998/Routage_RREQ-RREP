/********************
* HRLV EZ Max sonar 
* Reading analog voltage
* http://www.maxbotix.com
****************/

int sonarPin = A0;
float mm = 0.0;

void setup() {
  Serial.begin(38400);
}

void loop() {
  mm = analogRead(sonarPin) * 5; //Takes bit count and converts it to mm     
  // inches = mm/25.4 => Takes mm and converts it to inches
  Serial.print(mm);
  Serial.println("mm");
  delay(100);
}

